<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="575.0" prefWidth="478.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.InventoryManagement">
   <children>
      <TableView fx:id="productList" layoutX="42.0" layoutY="89.0" prefHeight="422.0" prefWidth="395.0">
        <columns>
            <TableColumn fx:id="supplierName_Column" prefWidth="129.0" text="Supplier" />
          <TableColumn fx:id="productname_Column" prefWidth="134.0" text="Product Name" />
            <TableColumn fx:id="currentProd_Column" prefWidth="67.0" text="Curr Amt" />
            <TableColumn fx:id="requiredProd_Column" prefWidth="64.0" text="Req. Amt" />
        </columns>
      </TableView>
      <Label layoutX="160.0" layoutY="14.0" prefHeight="53.0" prefWidth="160.0" text="Inventory">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button layoutX="42.0" layoutY="523.0" mnemonicParsing="false" onAction="#handleEditProduct" prefHeight="40.0" prefWidth="51.0" text="Edit" />
      <Button layoutX="320.0" layoutY="523.0" mnemonicParsing="false" onAction="#handleDeleteProduct" prefHeight="40.0" prefWidth="51.0" text="Delete" />
      <Button layoutX="386.0" layoutY="523.0" mnemonicParsing="false" onAction="#handleAddEmployee" prefHeight="40.0" prefWidth="51.0" text="Add" />
      <Button layoutX="42.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBack" text="Back" />
      <Button fx:id="refresh_Button" layoutX="381.0" layoutY="14.0" mnemonicParsing="false" onAction="#loadDataFromDatabase" text="Refresh" />
   </children>
</AnchorPane>
